
<!DOCTYPE html>
<html>
<body>

<a href="/download">Baixar script.py</a>

</body>
</html>









from flask import Flask, send_file

app = Flask(__name__)

@app.route("/")
def index():
    return """
    <a href='/download'>Baixar script.py</a>
    """

@app.route("/download")
def download():
    return send_file("script.py", as_attachment=True)

if __name__ == "__main__":
    app.run(debug=True)








print  ("Porompopom")
